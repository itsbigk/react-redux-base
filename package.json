{
  "name": "node-react",
  "version": "0.0.1",
  "description": "Using node and react with webpack",
  "main": "server.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js; ./node_modules/.bin/babel server -d lib",
    "dev": "webpack --config webpack.config.dev.js; NODE_ENV=development nodemon index.js",
    "start": "webpack --config webpack.config.prod.js; NODE_ENV=production node ./lib/server.js",
    "test": "./node_modules/karma/bin/karma start",
    "tdd": "./node_modules/karma/bin/karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsbigk/node-react.git"
  },
  "author": "Kyle Knott",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/itsbigk/node-react/issues"
  },
  "homepage": "https://github.com/itsbigk/node-react#readme",
  "dependencies": {
    "body-parser": "^1.14.0",
    "express": "^4.13.3",
    "flux": "^2.1.1",
    "keymirror": "^0.1.1",
    "method-override": "^2.3.5",
    "mongoose": "^4.1.7",
    "morgan": "^1.6.1",
    "node-sass": "^3.4.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.6.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "expect.js": "^0.3.1",
    "file-loader": "^0.8.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-coverage": "^0.5.5",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.24",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.5",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
