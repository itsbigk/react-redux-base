{
  "name": "react-redux-base",
  "version": "0.0.3",
  "description": "React Redux base app with node and webpack",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean; webpack --config webpack.config.prod.js; webpack --config webpack.config.server.js",
    "dev": "concurrently \"nodemon index.js\" \"npm run dev:client\"",
    "dev:client": "webpack-dev-server --config webpack.config.dev.js",
    "start": "node ./dist/bundle.server.js",
    "start:fresh": "npm run build; node ./dist/bundle.server.js",
    "tdd": "karma start --auto-watch --no-single-run",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsbigk/react-redux-base.git"
  },
  "author": "Kyle Knott",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/itsbigk/react-redux-base/issues"
  },
  "homepage": "https://github.com/itsbigk/react-redux-base#readme",
  "dependencies": {
    "body-parser": "^1.14.0",
    "express": "^4.14.0",
    "kerberos": "0.0.21",
    "method-override": "^2.3.7",
    "mongoose": "^4.7.6",
    "qs": "^6.3.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^2.8.1",
    "redux": "^3.5.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "chunk-manifest-webpack-plugin": "^0.1.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "ignore-loader": "^0.1.1",
    "ignore-styles": "^5.0.1",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-nyan-reporter": "^0.2.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "morgan": "^1.6.1",
    "nodemon": "^1.10.2",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.11",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.2.1",
    "react-addons-test-utils": "^15.3.0",
    "redux-mock-store": "^1.1.2",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
